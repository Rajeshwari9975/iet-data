package com.demo.linkedlist;

public class Linkedlist {
	class Node{
		int data;
		Node next;
		public Node(int data, Node next) {
			super();
			this.data = data;
			this.next = null;
		}
		
	}
	
	Node head;

	public Linkedlist() {
		super();
		head = null;
	}
	Node temp = head;
	public void addElement(int num) {
		if(head == null) {
			Node node = new Node(num, head);
			head= node;
		}
		else {
			while(temp != null) {
				temp = temp.next;
			}
			temp.data = num;
		}
	}

	public void displayList() {
		while(temp != null) {
			System.out.print(temp.data+ " ");
			temp = temp.next;
		}
	}
	

}
