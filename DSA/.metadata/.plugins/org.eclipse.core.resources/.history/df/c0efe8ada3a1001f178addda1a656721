package com.demo.service;

public class MyLinkedList {
	
	class Node{
		String data;
		Node next;
		public Node(String data) {
			super();
			this.data = data;
			this.next = null;
		}
	}
	
	Node head;

	public MyLinkedList() {
		super();
		this.head = null;
	}
	
	public void addData(String vertex) {
		Node newNode = new Node(vertex);
		
		if(head == null) {
			head = newNode;
		}
	}
	
	

}
